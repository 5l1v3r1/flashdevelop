<?xml version="1.0" encoding="utf-8"?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<keyword-classes>
		<keyword-class name="csharp-primary-keywords">
			abstract as base bool break by byte case catch char checked class const continue decimal default
			delegate do double descending explicit event extern else enum false finally fixed float for foreach
			from goto group if implicit in int interface internal into is lock long new null namespace object
			operator out override orderby params private protected public readonly ref return switch struct sbyte
			sealed short sizeof stackalloc static string select this throw true try typeof uint ulong unchecked
			unsafe ushort using var virtual volatile void while where yield
		</keyword-class>
		<keyword-class name="csharp-additional-keywords">
			Boolean Byte Char DateTime Decimal Double Int16 Int32 Int64 IntPtr SByte Single UInt16 UInt32 UInt64 UIntPtr Void
		</keyword-class>
	</keyword-classes>
	<languages>
		<language name="csharp">
			<lexer name="cpp" style-bits="7" />
			<character-class><![CDATA[qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM0123456789_$@]]></character-class>
			<line-comment><![CDATA[//]]></line-comment>
			<comment-start><![CDATA[/*]]></comment-start>
			<comment-end><![CDATA[*/]]></comment-end>
			<file-extensions>cs</file-extensions>
			<use-keywords>
				<keyword key="1" class="csharp-additional-keywords" />
				<keyword key="3" class="csharp-primary-keywords" />
			</use-keywords>
			<!-- COLORING_START -->
			<editor-style caret-fore="0xFFFFFF" caretline-back="0xFBC404" selection-fore="0x000000" selection-back="0x8E8775" margin-fore="0x302d26" margin-back="0x302d26" highlight-back="0xffffff" marker-fore="0x302d26" marker-back="0x726A65" colorize-marker-back="true" />
			<use-styles>
				<style name="default" fore="0xf9f9f9" back="0x302d26" size="9" font="Consolas,Courier New,monospace" />
				<style name="comment" fore="0x8F8B76" />
				<style name="commentline" fore="0x8F8B76" />
				<style name="commentdoc" fore="0x8F8B76" />
				<style name="number" fore="0x7bc654" />
				<style name="word" fore="0xb6a129" />
				<style name="string" fore="0xd38b38" />
				<style name="character" fore="0xEEA382" />
				<style name="uuid" />
				<style name="preprocessor" />
				<style name="operator" />
				<style name="identifier" />
				<style name="stringeol" />
				<style name="verbatim" />
				<style name="regex" fore="0xACD2C7" />
				<style name="commentlinedoc" fore="0x8F8B76" />
				<style name="word2" fore="0xFFFFC1" />
				<style name="word3" fore="0xffee80" />
				<style name="word4" fore="0xffee80" />
				<style name="word5" fore="0xffee80" />
